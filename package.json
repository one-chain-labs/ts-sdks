{
	"name": "@onelabs/ts-sdks",
	"private": true,
	"license": "Apache-2.0",
	"scripts": {
		"test": "turbo run test",
		"test:e2e": "turbo run test:e2e --concurrency=1",
		"icons": "pnpm --filter ./apps/icons",
		"wallet": "turbo --filter ./apps/wallet",
		"bcs": "turbo --filter ./packages/bcs",
		"changeset-version": "pnpm changeset version && pnpm --filter @onelabs/sui codegen:version",
		"prettier:check": "prettier -c --ignore-unknown .",
		"prettier:fix": "prettier -w --ignore-unknown .",
		"prettier:fix:watch": "onchange '**' -i -f add -f change -j 5 -- prettier -w --ignore-unknown {{file}}",
		"eslint:check": "eslint --max-warnings=0 .",
		"eslint:fix": "pnpm run eslint:check --fix",
		"lint": "pnpm run eslint:check && pnpm run prettier:check",
		"lint:fix": "pnpm run eslint:fix && pnpm run prettier:fix",
		"build": "turbo run build",
		"publish": "pnpm changeset-version && npx changeset publish"
	},
	"pnpm": {
		"overrides": {
			"node-notifier": "10.0.0",
			"async": "3.2.2",
			"nth-check": "2.0.1",
			"yaml@<2.2.2": ">=2.2.2",
			"semver@<7.5.2": ">=7.5.2",
			"postcss@<8.4.31": ">=8.4.31",
			"dompurify@>=3.0.0 <3.1.3": ">=3.1.3",
			"cross-spawn@5.1.0": "7.0.5"
		}
	},
	"engines": {
		"pnpm": ">=9.0.0"
	},
	"manypkg": {
		"ignoredRules": [
			"INTERNAL_MISMATCH",
			"ROOT_HAS_DEV_DEPENDENCIES"
		]
	},
	"devDependencies": {
		"@changesets/cli": "^2.28.1",
		"@ianvs/prettier-plugin-sort-imports": "^4.4.1",
		"@manypkg/cli": "^0.23.0",
		"@tanstack/eslint-plugin-query": "^5.68.0",
		"@testcontainers/postgresql": "^10.23.0",
		"@typescript-eslint/eslint-plugin": "^8.19.0",
		"@typescript-eslint/parser": "^8.19.0",
		"concurrently": "^9.1.2",
		"eslint": "^8.45.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-config-react-app": "^7.0.1",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-header": "^3.1.1",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-require-extensions": "^0.1.3",
		"eslint-plugin-tsdoc": "^0.2.17",
		"eslint-plugin-unused-imports": "^3.0.0",
		"graphql-config": "^5.0.3",
		"prettier": "^3.5.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"testcontainers": "^10.23.0",
		"tsx": "^4.19.3",
		"turbo": "^2.4.4",
		"typescript": "^5.8.2"
	},
	"packageManager": "pnpm@9.1.1+sha512.14e915759c11f77eac07faba4d019c193ec8637229e62ec99eefb7cf3c3b75c64447882b7c485142451ee3a6b408059cdfb7b7fa0341b975f12d0f7629c71195"
}
